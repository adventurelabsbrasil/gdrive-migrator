
{
  "manifest_version": 3,
  "name": "DriveMigrator Pro",
  "version": "1.0",
  "description": "Migre arquivos entre contas Google Drive com facilidade.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "identity",
    "storage"
  ],
  "host_permissions": [
    "https://www.googleapis.com/*",
    "https://accounts.google.com/*"
  ]
}
